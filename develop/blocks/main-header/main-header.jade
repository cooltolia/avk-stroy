        +e.top-row
            +e.A.logo&attributes(attributes)
                img(src='images/logo.svg' width='95' alt='АВK-Строй')
        +e.navigation
            +e.inner-row
                +main-nav
                +e.tel <span>тел.:</span> +7(812) 240-20-68